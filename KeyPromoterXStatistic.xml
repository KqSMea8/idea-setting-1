<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Method...">
        <Item>
          <IdeaActionID>ExtractMethod</IdeaActionID>
          <ShortCut>'⌥⌘M'</ShortCut>
          <Description>Method...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <Description>Implementation(s)</Description>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Test.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'Test.main()'</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>